  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "parcel": "^2.14.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"

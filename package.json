{
  "name": "crudp",
  "version": "1.0.0",
  "description": "## This  project shows the crud operation Focus area to create this ui design in typescript is:-",
 "type": "module",
  "scripts": {
    "dev": "parcel serve ./src/index.html --port 5500",
    "type-check": "tsc -b --noEmit",
    "lint": "eslint src --ext .ts --cache --fix",
    "build": "rm -rf dist .parcel-cache && npm run type-check && npm run lint && parcel build ./src/index.html --no-autoinstall --no-optimize",
    "format": "prettier src --write"
  },
  
